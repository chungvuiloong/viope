var num;
var totalPrime = 0;

	for ( num = 1; num <= 60; num++) {
		let flag = 0;

    // looping through 2 to user input number
    	for (let j = 2; j < num; j++) {
        if (num % j == 0) {
            flag = 1;
            break;
        }
    }

    // if number greater than 1 and not divisible by other numbers
    if (num > 1 && flag == 0) {
		totalPrime++;
        console.log(num);
    }
  
	

		
	}

console.log("Total of prime numbers found: " + totalPrime);
